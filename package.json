{
  "name": "meal_booking_app",
  "version": "1.0.0",
  "description": "An app to help book meals",
  "main": "index.js",
  "git repository": "https://github.com/benneee/MealApp",
  "scripts": {
    "test": "mocha api/src/tests/*.test.js --recursive --require @babel/register --timeout 10000 --exit",
    "test-watch": "nodemon --exec \"npm test\"",
    "tests": "mocha -- api/src/tests/*.test.js",
    "start": "nodemon --exec babel-node src/index",
    "babel-build": "babel src -d dist",
    "clean": "rm -rf dist && mkdir dist",
    "build": "npm run clean && npm run babel-build"
  },
  "engine": {
    "node": "10.13.0"
  },
  "heroku-run-build-script": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Benneee/MealApp.git"
  },
  "keywords": [],
  "author": "benedict nkeonye",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "sequelize": "^4.42.0",
    "@babel/preset-env": "^7.3.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "expect": "^1.20.2",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.10",
    "supertest": "^3.4.2"
  }
}
