{
  "name": "meal_booking_app",
  "version": "1.0.0",
  "description": "An app to help book meals",
  "main": "index.js",
  "git repository": "https://github.com/benneee/MealApp",
  "scripts": {
    "test": "mocha src/tests/**.test.js --recursive --require @babel/register --timeout 10000 --exit",
    "test-watch": "nodemon --exec \"npm test\"",
    "start": "node build/dist/index.js",
    "dev-start": "nodemon --exec babel-node src/index",
    "babel-build": "babel src -d dist",
    "clean": "rm -rf dist && mkdir dist",
    "build": "npm run clean && npm run babel-build",
    "generate-lcov": "nyc report --reporter=text-lcov > lcov.info",
    "coveralls-coverage": "coveralls < lcov.info",
    "codeclimate-coverage": "codeclimate-test-reporter < lcov.info",
    "coverage": "nyc npm run test && npm run generate-lcov && npm run coveralls-coverage && npm run codeclimate-coverage"
  },
  "engine": {
    "node": "10.13.0"
  },
  "heroku-run-build-script": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Benneee/MealApp.git"
  },
  "keywords": [],
  "author": "benedict nkeonye",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "sequelize": "^4.42.0",
    "@babel/preset-env": "^7.3.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "codeclimate-test-reporter": "^0.5.1",
    "coveralls": "^3.0.3",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "expect": "^1.20.2",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0",
    "supertest": "^3.4.2"
  },
  "bugs": {
    "url": "https://github.com/Benneee/MealApp/issues"
  },
  "homepage": "https://github.com/Benneee/MealApp#readme"
}
